# platform :ios, '9.0'

target 'ChicagoComedy' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ChicagoComedy
  pod "PromiseKit", "~> 4.4"
  pod 'Alamofire', "~> 4.5"
